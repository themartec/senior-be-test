{
  "name": "themartec_backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "dev": "concurrently \"npm run watch\" \"nodemon -q dist/main.js\"",
    "build": "webpack --mode=production --define-process-env-node-env=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --watch",
    "lint": "eslint . --ext .ts",
    "migration:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/database.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/database.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "googleapis": "^109.0.1",
    "helmet": "^6.0.0",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/http-errors": "^2.0.1",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@webpack-cli/generators": "^3.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.5.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "description": "The Martec Test"
}
